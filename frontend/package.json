{
    "name": "frontend",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "cross-env VITE_ENVIRON=devlopment vite",
        "qa": "cross-env VITE_ENVIRON=qa vite",
        "prod": "cross-env VITE_ENVIRON=production vite",
        "build": "cross-env VITE_ENVIRON=production vite",
